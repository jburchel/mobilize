{
  "name": "mobilize-crm",
  "version": "1.0.0",
  "description": "Mobilize CRM - Customer Relationship Management for Mobilization",
  "scripts": {
    "css:dev": "cross-env NODE_ENV=development postcss app/static/css/*.css --dir app/static/css/dist/ --config postcss.config.js --watch",
    "css:build": "cross-env NODE_ENV=production postcss app/static/css/*.css --dir app/static/css/dist/ --config postcss.config.js --no-map",
    "images:optimize": "node scripts/optimize-images.js",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "export FLASK_APP=app.py && export FLASK_ENV=development && export FLASK_DEBUG=1 && concurrently \"npm run css:dev\" \"python3.11 app.py\""
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@chromatic-com/storybook": "^3.2.6",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@storybook/addon-a11y": "^8.6.9",
    "@storybook/addon-actions": "^8.6.9",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-links": "^8.6.9",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.5",
    "@storybook/html": "^8.6.9",
    "@storybook/html-webpack5": "^8.6.9",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^8.0.0",
    "lit-html": "^3.2.1",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-critical-split": "^2.5.3",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.4.2",
    "purgecss": "^5.0.0",
    "sharp": "^0.32.1",
    "storybook": "^8.6.9"
  }
}
